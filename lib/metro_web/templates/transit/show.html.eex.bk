<h2>Show Transit</h2>

<ul>

  <li>
    <strong>Estimated arrival:</strong>
    <%= @transit.estimated_arrival %>
  </li>

  <li>
    <strong>Actual arrival:</strong>
    <%= @transit.actual_arrival %>
  </li>

</ul>

<span><%= link "Edit", to: transit_path(@conn, :edit, @transit) %></span>
<span><%= link "Back", to: transit_path(@conn, :index) %></span>
