<h2>Show Event</h2>

<ul>

  <li>
    <strong>Description:</strong>
    <%= @event.description %>
  </li>

  <li>
    <strong>Images:</strong>
    <%= @event.images %>
  </li>

  <li>
    <strong>Datetime:</strong>
    <%= @event.datetime %>
  </li>

</ul>

<span><%= link "Edit", to: event_path(@conn, :edit, @event) %></span>
<span><%= link "Back", to: event_path(@conn, :index) %></span>
